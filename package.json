{
  "name": "node-express-starter",
  "version": "1.0.0",
  "main": "index.js",
  "author": "@Raed Fadhlaoui <raed.fadhlaoui@hotmail.com>",
  "description": "Express, MongoDB, Firebase, and Swagger for API documentation starter for building RESTful APIs",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production && node index.js",
    "develop": "SET NODE_ENV=development && nodemon index.js",
    "lint:check": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "format:fix": "prettier --write .",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:dry": "standard-version --dry-run",
    "prepush": "npm run format:fix && npm run lint:check && npm run format:check",
    "prepare": "husky install"
  },
  "keywords": [
    "node.js",
    "express.js",
    "swagger",
    "mongodb",
    "restful-api"
  ],
  "dependencies": {
    "async": "^3.2.3",
    "babel-eslint": "^10.1.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.7.3",
    "nodemon": "^3.1.0",
    "prettier": "^2.6.2",
    "standard-version": "^9.3.2",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2"
  },
  "engines": {
    "node": ">=14.16.0 <=16.13.0",
    "npm": ">=6.14.11 <=8.1.0"
  }
}
