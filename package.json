{
  "name": "express-node-starter",
  "version": "1.7.0",
  "main": "server.js",
  "author": "@Raed Fadhlaoui <raed.fadhlaoui@hotmail.com>",
  "description": "Express, MongoDB, Firebase, and Swagger for API documentation starter for building RESTful APIs",
  "license": "MIT",
  "bin": {
    "express-node-starter": "./boilerplate/cli/crudOperation.js",
    "create-express-node-starter": "./boilerplate/cli/index.js"
  },
  "scripts": {
    "start": "NODE_ENV=production && node server.js",
    "develop": "SET NODE_ENV=development && nodemon server.js",
    "develop:mac": "NODE_ENV=development && nodemon server.js",
    "crud-operation": "node boilerplate/cli/crudOperation.js generate",
    "lint:check": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "format:fix": "prettier --write .",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:dry": "standard-version --dry-run",
    "prepush": "npm run format:fix && npm run lint:check && npm run format:check",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fadhlaouir/express-node-starter.git"
  },
  "keywords": [
    "node.js",
    "express.js",
    "swagger",
    "mongodb",
    "restful-api"
  ],
  "dependencies": {
    "async": "^3.2.3",
    "babel-eslint": "^10.1.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^8.0.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.7.3",
    "nodemon": "^3.1.0",
    "npm": "^10.5.0",
    "prettier": "^2.6.2",
    "standard-version": "^9.3.2",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.16.0 <=20.11.0",
    "npm": ">=6.14.11 <=10.2.4"
  }
}
